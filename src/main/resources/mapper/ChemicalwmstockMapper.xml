<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.halcyon.file_manage.dao.ChemicalwmstockMapper">
	<resultMap id="BaseResultMap"
		type="com.halcyon.file_manage.model.Chemicalwmstock">
		<!-- WARNING -->
		<id column="id" jdbcType="INTEGER" property="id" />
		<result column="Plant" jdbcType="VARCHAR" property="plant" />
		<result column="StorageBin" jdbcType="VARCHAR"
			property="storagebin" />
		<result column="MaterialNo" jdbcType="VARCHAR"
			property="materialno" />
		<result column="TotalQuantity" jdbcType="DECIMAL"
			property="totalquantity" />
		<result column="Description" jdbcType="VARCHAR"
			property="description" />
		<result column="Baseunit" jdbcType="VARCHAR"
			property="baseunit" />
	</resultMap>


	<select id="selectAll" resultMap="BaseResultMap">
		select * from
		local_sap_data.ChemicalWMStock
		where 1=1 order by StorageBin
	</select>

</mapper>